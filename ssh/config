Host *
	IgnoreUnknown AddKeysToAgent,UseKeychain
	AddKeysToAgent yes
	UseKeychain yes

Host github.com
	IdentitiesOnly yes

Match host private.github.com user git
	HostName github.com
	IdentityFile ~/.ssh/keys/github

Match host github.com user git
	IdentityFile ~/.ssh/keys/github

# vim: ft=sshconfig
